            <!-- MENU SECTION -->
            <div id="left">
                <!-- User Profile Pic -->
                {{#if isUserLoggedIn}}

                <div class="media user-media well-small">
                    <a class="user-link">
                        <img class="media-object img-thumbnail user-img" alt="User Picture"
                             src="/users/{{user.userPicName}}" />
                    </a>
                    <br />
                    <div class="media-body">
                        <h5 class="media-heading">{{user.fullName}}</h5>
                        <ul class="list-unstyled user-info">
                            <li><a class="btn btn-success btn-xs btn-circle" style="width: 10px;height: 12px;"></a> Online</li>
                        </ul>
                    </div>
                    <br />
                </div>
                {{/if}}

                <!-- Left Side Navigation -->
            <ul id="menu" class="collapse">

                {{#if isAdmin}}
                <li class="panel ">
                    <a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle"
                       data-target="#component-admin">
                        <i class="icon-tasks"> </i> Admin
                        <span class="pull-right"> <i class="icon-double-angle-down"></i> </span>
                        <!--&nbsp; <span class="label label-default">10</span>&nbsp;-->
                    </a>
                        <ul class="collapse" id="component-admin">
                            <li ><a href="/users"><i class="icon-arrow-right"></i> Users </a></li>
                            <li ><a href="/articles"><i class="icon-arrow-right"></i> Articles </a></li>
                            <li ><a href="/courses"><i class="icon-arrow-right"></i> Courses </a></li>
                            <li ><a href="/questions"><i class="icon-arrow-right"></i> Questions </a></li>
                            <li ><a href="/suggestions"><i class="icon-arrow-right"></i> Suggestions </a></li>
                    </ul>
                </li>
                {{/if}}

                <li class="panel ">
                    <a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle"
                       data-target="#component-courses">
                        <i class="icon-tasks"> </i> Courses
                        <span class="pull-right"> <i class="icon-double-angle-down"></i> </span>
                    </a>
                    <ul class="collapse" id="component-courses">

                        <li ><a href="/courses/1"><i class="icon-arrow-right"></i> Java Language</a></li>
                        <li ><a href="/courses"><i></i> See More Courses ...</a></li>
                    </ul>
                </li>

                <li class="panel ">
                    <a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle"
                       data-target="#component-articles">
                        <i class="icon-tasks"> </i> Articles
                        <span class="pull-right"> <i class="icon-double-angle-down"></i> </span>
                    </a>
                    <ul class="collapse" id="component-articles">

                        <li ><a href="/articles/1"><i class="icon-arrow-right"></i> How to be A programmer</a></li>
                        <li ><a href="/articles"><i></i> See More Articles ...</a></li>
                    </ul>
                </li>

                <li class="panel ">
                    <a href="#" data-parent="#menu" data-toggle="collapse" class="accordion-toggle"
                       data-target="#component-questions">
                        <i class="icon-tasks"> </i> Questions
                        <span class="pull-right"> <i class="icon-double-angle-down"></i> </span>
                    </a>
                    <ul class="collapse" id="component-questions">

                        <li ><a href="/questions/ask"><i class="icon-arrow-right"></i> Ask a Question</a></li>
                        <li ><a href="/questions/common"><i class="icon-arrow-right"></i> Common Questions</a></li>
                    </ul>
                </li>
                <li class="panel">
                    <a href="/suggestions" ><i class="icon-tasks"></i>  Suggestions </a>
                </li>

            </ul>
