    <!-- BEGIN BODY -->
    <body class="padTop53 ">

        <!-- MAIN WRAPPER -->
        <div id="wrap">

            <!-- HEADER SECTION -->
            <div id="top">
                <nav class="navbar navbar-inverse navbar-fixed-top " style="padding-top: 10px;">
                    <a data-original-title="Show/Hide Menu" data-placement="bottom" data-tooltip="tooltip" href="#menu"
                            class="accordion-toggle btn btn-primary btn-sm visible-xs" data-toggle="collapse" id="menu-toggle">
                        <i class="icon-align-justify"></i>
                    </a>

                    <!-- LOGO SECTION -->
                    <div class="row">
                        <a href="/" class="navbar-brand" style="padding-left: 70px;padding-top: 5px;">Ahmed Mater</a>

                        <div class="btn-group col-sm-offset-1" >
                            <button data-toggle="dropdown" class="btn btn-info dropdown-toggle">Courses
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" >
                                <li><a href="/courses/1">Java Language</a></li>
                                <li class="divider"></li>
                                <li><a href="#">See More Courses ...</a></li>
                            </ul>
                        </div>


                        <div class="btn-group">
                            <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle">Articles
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" >
                                <li><a href="/articles/1">Action</a></li>
                                <li class="divider"></li>
                                <li><a href="/articles">See More Articles ....</a></li>
                            </ul>
                        </div>

                        <div class="btn-group">
                            <button data-toggle="dropdown" class="btn btn-danger dropdown-toggle">Questions
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" >
                                <li><a href="/questions/ask">Ask a Question</a></li>
                                <li><a href="/questions/common">Common Questions</a></li>
                            </ul>
                        </div>
                        <a class="btn btn-success" href="/suggestions">Suggestions</a>
                        <a class="btn btn-warning" href="/aboutMe">About Me</a>
                        <a class="btn btn-info" href="/contactMe">Contact Me</a>

                        {{#if isAdmin}}
                            <div class="btn-group">
                                <button data-toggle="dropdown" class="btn btn-success dropdown-toggle">Admin
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu" >
                                    <li><a href="/users">Users</a></li>
                                    <li><a href="/articles">Articles</a></li>
                                    <li><a href="/courses">Courses</a></li>
                                    <li><a href="/questions">Questions</a></li>
                                    <li><a href="/suggestions">Suggestions</a></li>
                                </ul>
                            </div>
                        {{/if}}
                    <!-- END LOGO SECTION -->
                    <!-- Top Right Navigation Icons --><!-- END LOGO SECTION -->

                        {{#if isUserLoggedIn}}
                        <ul class="nav navbar-top-links navbar-right">

                        <!--Notification SECTION -->
                        <li class="chat-panel dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <span class="label label-info">8</span>   <i class="ui-icon-notice"></i>&nbsp; <i class="icon-chevron-down"></i>
                            </a>

                            <ul class="dropdown-menu dropdown-alerts">

                                <li>
                                    <a href="#">
                                        <div>
                                            <i class="icon-comment" ></i> New Comment
                                            <span class="pull-right text-muted small"> 4 minutes ago</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <div>
                                            <i class="icon-twitter info"></i> 3 New Follower
                                            <span class="pull-right text-muted small"> 9 minutes ago</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <div>
                                            <i class="icon-envelope"></i> Message Sent
                                            <span class="pull-right text-muted small" > 20 minutes ago</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <div>
                                            <i class="icon-tasks"></i> New Task
                                            <span class="pull-right text-muted small"> 1 Hour ago</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <div>
                                            <i class="icon-upload"></i> Server Rebooted
                                            <span class="pull-right text-muted small"> 2 Hour ago</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a class="text-center" href="#">
                                        <strong>See All Alerts</strong>
                                        <i class="icon-angle-right"></i>
                                    </a>
                                </li>
                            </ul>

                        </li>
                        <!-- END Notification SECTION -->

                        <!--User SECTIONS -->
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="icon-user "></i>&nbsp; <i class="icon-chevron-down "></i>
                            </a>

                            <ul class="dropdown-menu dropdown-user">
                                <li><a href="user/{{user.userID}}/profile"><i class="icon-user"></i> User Profile </a>
                                </li>
                                <!--<li><a href="#"><i class="icon-gear"></i> Settings </a>-->
                                <!--</li>-->
                                <li class="divider"></li>
                                <li><a href="/logout"><i class="icon-signout"></i> Logout </a>
                                </li>
                            </ul>

                        </li>
                        <!--END User SECTION -->
                    </ul>
                    {{else}}

                        <a class="btn btn-primary col-lg-offset-3" href="/register"> Sign Up</a>
                        <a class="btn btn-success" href="/login"> login</a>
                        <!--<form>-->
                            <!--<ul class="nav navbar-top-links navbar-right">-->
                                <!--<li> <button class="btn text-muted text-center btn-metis-4" formaction="/register"> Sign Up</button> </li>-->
                                <!--<li> <button class="btn text-muted text-center btn-metis-1" formaction="/login"> Log in</button> </li>-->
                            <!--</ul>-->
                        <!--</form>-->
                    {{/if}}

                    </div>
                </nav>
            </div>
